---
import Categories from "./Categories.astro"
import Languages from "./Languages.astro"

interface Props {
  slug: string
}

const { slug } = Astro.props
---

<div class="mx-auto mt-12 max-w-screen-md px-4 sm:mt-16 md:px-8" id="details">
  <Languages slug={slug} />
  <Categories slug={slug} />
</div>
