  "devDependencies": {
    "@astrojs/check": "^0.9.3",
    "@biomejs/biome": "1.8.3",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7",
    "astro": "^4.15.2",
    "typescript": "^5.5.4"
  }
