  "scripts": {
    "dev": "pnpm --filter=sk8* dev",
    "build": "pnpm typecheck && pnpm --filter=sk8* build",
    "admin": "cd examples/sk8-ministries && pnpx decap-server",
    "typecheck": "pnpm --filter='@example/*' astro sync && astro check",
